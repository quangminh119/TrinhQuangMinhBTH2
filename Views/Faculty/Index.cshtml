@model IEnumerable<TrinhQuangMinhBTH2.Models.Faculty>

    @{
    ViewData["Title"] = "Index";
    }

    <h1>Index</h1>

    <p>
        <a class="btn btn-success" asp-action="Create">Create New</a>
    </p>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.FacultyName)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model) {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FacultyName)
                </td>
                <td>
                    <a class="btn btn-success" asp-action="Edit" asp-route-id="@item.FacultyID">Edit</a> |
                    <a class="btn btn-primary" asp-action="Details" asp-route-id="@item.FacultyID">Details</a> |
                    <a class="btn btn-danger" asp-action="Delete" asp-route-id="@item.FacultyID">Delete</a>
                </td>
            </tr>
}
        </tbody>
    </table>
